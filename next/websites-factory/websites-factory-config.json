{
    "modules": [
        {
            "name": "welcome",
            "enabled": true
        },
        {
            "name": "helloWorld",
            "enabled": true
        }
    ],
    "template": {
        "/": {
            "module": "welcome",
            "page": "home",
            "sections": [
                {
                    "module": "welcome",
                    "name": "introduction"
                }
            ],
            "layout": {
                "module": "welcome",
                "name": "main"
            },
            "entities": [
                "websitesFactory"
            ],
            "metadata": {
                "title": "{{websitesFactory.name}} | Welcome!",
                "description": "This is the example homepage of {{websitesFactory.name}}"
            }
        },
        "hello-world": {
            "module": "helloWorld",
            "page": "hello",
            "layout": {
                "module": "welcome",
                "name": "main"
            },
            "subPage": {
                "subPage-exemple": {
                    "module": "welcome",
                    "page": "home",
                    "sections": [
                        {
                            "module": "welcome",
                            "name": "introduction"
                        }
                    ]
                }
            }
        }
    },
    "entities": {
        "websitesFactory": "websitesFactory"
    }
}